<launch>
  <!-- â¬‡ï¸ åƒæ•¸èˆ‡ pano stitching ç›¸é—œ argsï¼ˆä½ çš„ç¬¬ä¸€ä»½ï¼‰ -->
  <arg name="left_topic"               default="/camera1_fix/color/image_raw/compressed"/>
